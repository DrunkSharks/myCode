<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>Project</display-name>
	<servlet>
		<servlet-name>ShequrecServlet</servlet-name>
		<servlet-class>com.gec.project.servlet.ShequrecServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ShequrecServlet</servlet-name>
		<url-pattern>/ShequrecServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ShequXinwenServlet</servlet-name>
		<servlet-class>com.gec.project.servlet.ShequXinwenServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ShequXinwenServlet</servlet-name>
		<url-pattern>/ShequXinwenServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ProductServlet</servlet-name>
		<servlet-class>com.gec.project.servlet.ProductServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProductServlet</servlet-name>
		<url-pattern>/ProductServlet</url-pattern>
	</servlet-mapping>

